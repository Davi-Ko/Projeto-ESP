.ORG 0X000

INICIO:

    LDI R16, 0B11111111
    OUT DDRD, R16

    LDI R16, 0B00000000
    OUT DDRC, R16
    
    LDI R16, 0B11111111
    OUT PORTC, R16

LOOP:

    SBIC PINC, PC2
    RJMP DESLIGA_LED
    RJMP LIGA_LED

DESLIGA_LED:

    CBI PORTD, PD5
    RJMP LOOP

LIGA_LED:

    SBI PORTD, PD5
    RJMP LOOP
